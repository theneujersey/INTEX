<!-- Page Heading -->
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            Welcome, Rebecca!
        </h1>
        <ol class="breadcrumb">
            <li>
                <i class="fa fa-dashboard"></i>  @Html.ActionLink("Dashboard","Index","Home")
            </li>
            <li class="active">
                <i class="fa fa-home"></i> Home
            </li>
        </ol>
    </div>
</div>
<!-- /.row -->

<div class="row">
    <div class="box col-lg-5">
        <h2>Work Orders</h2>
        <table class="table">
            <tr>
                <th>Work Orders</th>
                <th>Due Date</th>
            </tr>
            <tr class="danger">
                <td>@Html.ActionLink("#57814", "Review", "Home", null, new { @class="bold"})</td>
                <td>ABC Company</td>
                <td>12/2/16</td>
            <tr class="warning">
                <td>@Html.ActionLink("#57813", "Review", "Home", null, new { @class = "bold" })</td>
                <td>Acme Co.</td>
                <td>12/3/16</td>
            </tr>
            <tr class="success">
                <td>@Html.ActionLink("#57812", "Review", "Home", null, new { @class = "bold" })</td>
                <td>Company Ltd.</td>
                <td>12/4/16</td>
            </tr>
            <tr class ="success">
                <td>@Html.ActionLink("#57811", "Review", "Home", null, new { @class = "bold" })</td>
                <td>XYZ Company</td>
                <td>12/5/16</td>
            </tr>
        </table>

    </div>
    <div style="padding-bottom:20px;" class="box col-lg-5 col-lg-offset-1">
        <h2>Other notifications</h2>
        <img width="100%" src="~/Content/Images/chart.png" />
    </div>
</div>
<!-- /.row -->