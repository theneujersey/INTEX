<!-- Page Heading -->
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            Welcome, Rebecca!
        </h1>
        <ol class="breadcrumb">
            <li>
                <i class="fa fa-dashboard"></i>  @Html.ActionLink("Dashboard","Index","Home")
            </li>
            <li class="active">
                <i class="fa fa-home"></i> Home
            </li>
        </ol>
    </div>
</div>
<!-- /.row -->

<div class="row">
    <div class="box col-lg-5">
        <h2>Your schedule</h2>
        <table class="table">
            <tr>
                <th>Task Name</th>
                <th>Due</th>
            </tr>
            <tr class="danger">
                <td>Enter day's compounds</td>
                <td>12/2/16</td>
            <tr class="warning">
                <td>Run a batch of tests</td>
                <td>12/3/16</td>
            </tr>
            <tr class="success">
                <td>Attend a meeting</td>
                <td>12/4/16</td>
            </tr>
            <tr class ="success">
                <td>Weekly Review</td>
                <td>12/5/16</td>
            </tr>
        </table>

    </div>
    <div style="padding-bottom:20px;" class="box col-lg-5 col-lg-offset-1">
        <h2>Other notifications</h2>
        <img width="100%" src="~/Content/Images/chart.png" />
    </div>
</div>
<!-- /.row -->