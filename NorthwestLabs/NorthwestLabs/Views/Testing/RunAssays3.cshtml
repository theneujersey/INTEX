@model NorthwestLabs.Models.TestTubeSchedule

@{
    ViewBag.Title = "RunAssays3";
}

<div class="row">
    <h2>Complete Assay 1</h2>

    <p>Enter the data from Assay 1 here to complete the test.</p>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-horizontal">
            @Html.ValidationSummary(true)
            <div class="form-group">
                @Html.LabelFor(model => model.testTube.quantitativeResult , htmlAttributes: new { @class = "control-label col-md-2" })
                <div class="col-md-10">
                    @Html.EditorFor(model => model.testTube.quantitativeResult)
                    @Html.ValidationMessageFor(model => model.testTube.quantitativeResult)
                </div>
            </div>
            <div class="form-group">
                @Html.LabelFor(model => model.testTube.qualitativeResult, htmlAttributes: new { @class = "control-label col-md-2" })
                <div class="col-md-10">
                    @Html.EditorFor(model => model.testTube.qualitativeResult)
                    @Html.ValidationMessageFor(model => model.testTube.qualitativeResult)
                </div>
            </div>
            <div class="form-group">
                @Html.LabelFor(model => model.testTube.comments, htmlAttributes: new { @class = "control-label col-md-2" })
                <div class="col-md-10">
                    @Html.EditorFor(model => model.testTube.comments)
                    @Html.ValidationMessageFor(model => model.testTube.comments)
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" value="Create" class="btn btn-default" />
                </div>
            </div>
        </div>
    }
</div>

