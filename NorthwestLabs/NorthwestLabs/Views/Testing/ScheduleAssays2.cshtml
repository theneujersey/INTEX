@model NorthwestLabs.Models.WorkOrderSchedule

@{
    ViewBag.Title = "ScheduleAssays2";
}

<!-- In the real system, this page would post information to the database schedule table -->

<h2>Schedule Assays</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        @Html.ValidationSummary(true)
        <div class="form-group">
            @Html.LabelFor(model => model.workOrder.woID , htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.workOrder.woID)
                @Html.ValidationMessageFor(model => model.workOrder.woID)
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.schedule.completionEstimatedDate, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.schedule.completionEstimatedDate)
                @Html.ValidationMessageFor(model => model.schedule.completionEstimatedDate)
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                @Html.ActionLink("Schedule", "ScheduleAssays3", "Testing", new { @class = "btn btn-primary" })
            </div>
        </div>
    </div>
}




